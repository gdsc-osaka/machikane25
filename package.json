{
	"name": "machikane25",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"dev:art-backend": "pnpm --filter art-backend dev",
		"dev:photo": "pnpm --filter photo dev -p 3000",
		"dev:stamp": "pnpm --filter stamp dev -p 3001",
		"build:photo": "pnpm --filter photo build",
		"build:photo-cleaner": "pnpm --filter photo-cleaner build",
		"build:fish-cleaner": "pnpm --filter fish-cleaner build",
		"build:stamp": "pnpm --filter stamp build",
		"build": "pnpm --recursive build",
		"start:photo": "pnpm --filter photo start",
		"start:photo-cleaner": "pnpm --filter photo-cleaner start",
		"start:fish-cleaner": "pnpm --filter fish-cleaner start",
		"start:stamp": "pnpm --filter stamp start",
		"lint": "pnpm --recursive lint && pnpm exec biome check *.ts *.json",
		"lint:fix": "pnpm --recursive lint:fix && pnpm exec biome check --write *.ts *.json",
		"test:art-backend": "pnpm --filter art-backend test",
		"test:art-backend:e2e": "pnpm --filter art-backend test:e2e",
		"test:photo": "pnpm --filter photo test",
		"test:photo-cleaner": "pnpm --filter photo-cleaner test",
		"test:fish-cleaner": "pnpm --filter fish-cleaner test",
		"test:stamp": "pnpm --filter stamp test",
		"test": "pnpm --recursive test",
		"coverage": "pnpm --recursive coverage",
		"prebuild": "pnpm run build:photo-cleaner && pnpm run build:fish-cleaner",
		"emulator": "pnpm run prebuild && firebase emulators:start",
		"emulator:bg": "pnpm run prebuild && firebase emulators:start &",
		"deploy:fire": "pnpm exec firebase deploy && pnpm --recursive deploy:fire",
		"deploy:fire:check": "pnpm run prebuild && pnpm exec firebase deploy --dry-run"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@10.12.4",
	"devDependencies": {
		"@biomejs/biome": "2.2.6",
		"@eslint/eslintrc": "^3",
		"@firebase/rules-unit-testing": "^5.0.0",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.0",
		"@vitejs/plugin-react": "^5.0.4",
		"@vitest/coverage-v8": "^3.2.4",
		"dotenv": "^17.2.3",
		"eslint": "^9",
		"eslint-config-next": "15.5.5",
		"firebase-tools": "^14.19.1",
		"jsdom": "^27.0.0",
		"prettier": "3.6.2",
		"tsx": "^4.20.6",
		"typescript": "^5",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@sentry/nextjs": "^10.20.0"
	}
}
